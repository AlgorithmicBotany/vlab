description
rd.l
rd.v
rd.a
rd.map
rd.p
*
generate:
	cpfg -m rd.map rd.l rd.v rd.a
colourmap:
	loadmap:
		loadmap rd.map
	savemap:
		savemap rd.map
	PALETTE

l-system:
	EDIT rd.l
	panel:
		display:
			panel rd.p | awkped rd.l
		EDIT rd.p
view:
	EDIT rd.v
animate:
	EDIT rd.a
text:
	TEXTDISP description
