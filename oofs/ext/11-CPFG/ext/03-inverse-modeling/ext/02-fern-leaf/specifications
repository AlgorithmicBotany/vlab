	EDIT plant.l

view:
	EDIT plant.v

material:
	MEDIT plant.mat

functions:
	int_length:
		funcedit int_length.func
	int_width:
		funcedit int_width.func
	curvature:
		funcedit curvature.func
	leaf_length:
		funcedit leaf_length.func
	leaf_width:
		funcedit leaf_width.func
	branch_angle:
		funcedit brangle.func

contour:
	leaf:
		cuspy leaf.con

description:
	EDIT description.txt
